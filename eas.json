{
  "cli": {
    "version": ">= 0.60.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "buildConfiguration": "Debug",
        "simulator": true
      }
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "LANG": "en_US.UTF-8",
        "RCT_NEW_ARCH_ENABLED": "0",
        "EAS_SKIP_AUTO_FINGERPRINT": "1"
      },
      "ios": {
        "simulator": true,
        "image": "latest",
        "buildConfiguration": "Debug"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      }
    },
    "ios-fix": {
      "distribution": "store",
      "credentialsSource": "remote",
      "env": {
        "LANG": "en_US.UTF-8",
        "RCT_NEW_ARCH_ENABLED": "0",
        "EAS_SKIP_AUTO_FINGERPRINT": "1"
      },
      "ios": {
        "image": "latest",
        "resourceClass": "m-medium",
        "simulator": false,
        "buildConfiguration": "Release",
        "cocoapods": "1.14.3",
        "scheme": "Resonote"
      },
      "prebuildCommand": "find ./ios -name Podfile && cd ios && pod install"
    },
    "androidapk": {
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
